"),this.$eventualGainValue=this.$el.find(".ticket-details .ticket-eventual-gain.win div.ticket-eventual-gain-value"),this.$winTaxBox=this.$el.find(".ticket-details .ticket-eventual-gain.tax"),this.$winTaxValue=this.$winTaxBox.find("div.ticket-value"),this.$payoutBox=this.$el.find(".ticket-details .ticket-eventual-gain.payout"),this.$payoutValue=this.$payoutBox.find("div.ticket-value"),this.$ticketButton=this.$el.find("div.ticket-view"),this.$submitButton=this.$el.find(".ticket-submit a.ticket-btn-submit"),this.$confirmButton=this.$el.find(".betslip_submit_confirm"),this.$cancelButton=this.$el.find(".betslip_submit_cancel"),this.$showTicket=this.$el.find(".ticket-view"),this.messagesView.setElement(this.$messagesContainer)),this},showTicket:function(){return}});PSKweb.Betslip.BetslipContainerView=PSKweb.Betslip.BetslipView.extend({template:JST["Betslip/templates/betslip-container.hbs"],betslipViews:[],events:{"click .ticket-choose-btn":"onBetslipIndexClick","change .ticket-policy .checkbox input":"onPolicyChange"},onBetslipIndexClick:function(n){this.controller.setActiveBetslip($(n.target).data("index"))},onPolicyChange:function(n){this.controller.setAutoAcceptOddsChange($(n.currentTarget).prop("checked"));this.handleIsAutomaticChangeAcceptedChange()},initialize:function(n){var t=this;this.controller=n.controller;this.listenTo(this.model,"betslips:change:State",this.handleStateChange,this);this.listenTo(this.model,"change:ActiveBetslipIndex",this.updateBetslipChooser,this);this.listenTo(this.model,"change:IsAutomaticChangeAccepted",this.handleIsAutomaticChangeAcceptedChange,this);_.each(this.model.get("Betslips"),function(i){t.betslipViews.push(new PSKweb.Betslip.BetslipSportView({model:i,controller:n.controller,container:t}))})},render:function(){var n=this;return this.$el.html(this.template({acceptLiveOddsChange:PSKweb.Translations.betslip.acceptLiveOddsChange,betslipIndices:[1,2,3,4,5,6]})),this.$betslipChooser=this.$el.find(".ticket-choose-container"),this.$ticke